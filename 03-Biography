#BIOGRAPHY
#Kcijo Galapate


bio = {}
bio["name"] = input("What's your name?:")
bio["hometown"] = input("Where are you from?:")
bio["age"] = input("How old are you?:")
print("\n===============BIO STORED================")
for key, value in bio.items():
    print(f"{key}: {value}")
print("-------------------------------------------")
print(f"Hello {bio["name"]} nice to meet you! I see you're from {bio["hometown"]} and you're {bio["age"]} years old!")
